# where the root is relative to this file (optional, default = .)
root ..

# Configure the design pattern builder
extension dp com.gmmapowell.designpatterns.builder.DPConfigCommand

dp HelloAndroid dp/helloAndroid.xml

# where to find libraries - this really should be the default
libs lib

# Load maven files from the repo using the "Apache Buildr" format
maven junit:junit:jar:4.8.2

# aapt-gen, java, dex, aapt-package, apk 
android HelloAndroid
adbinstall HelloAndroid -emulator
